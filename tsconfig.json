{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "strictPropertyInitialization": false,
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "allowJs": true,
    // "paths": {
    //   "@modules/*": [
    //     "modules/*"
    //   ],
    //   "@config/*": [
    //     "config/*"
    //   ],
    //   "@shared/*": [
    //     "shared/*"
    //   ]
    // },
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
